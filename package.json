{
  "name": "aws-platform",
  "version": "0.1.0",
  "bin": {
    "aws-platform": "app/aws-platform.js"
  },
  "scripts": {
    "build:check": "tsc --noEmit",
    "test": "jest",
    "cdk:deploy": "tsc --noEmit && cdk deploy --all --context $(./scripts/getAwsSSOGroups.ts) --context $(./scripts/getRootOUId.sh) --context $(./scripts/getAwsOrgId.sh)",
    "cdk:synth": "tsc --noEmit && cdk synth --context $(./scripts/getAwsSSOGroups.ts) --context $(./scripts/getRootOUId.sh) --context $(./scripts/getAwsOrgId.sh)",
    "cdk:diff": "tsc --noEmit && cdk diff --all --context $(./scripts/getAwsSSOGroups.ts) --context $(./scripts/getRootOUId.sh) --context $(./scripts/getAwsOrgId.sh)",
    "lint:ci": "eslint . --ext .ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "typescript": "~5.2.2",
    "@types/node": "20.7.1",
    "@types/aws-lambda": "^8.10.126",
    "cdk": "2.110.1",
    "constructs": "^10.0.0",
    "aws-cdk": "2.110.1",
    "aws-cdk-lib": "2.110.1",
    "esbuild": "0.19.7",
    "eslint": "^8.54.0",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "prettier": "3.1.0",
    "ts-jest": "^29.1.1",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "tslog": "^4.9.2"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.460.0",
    "@aws-sdk/client-account": "^3.454.0",
    "@aws-sdk/client-identitystore": "3.454.0",
    "@aws-sdk/client-organizations": "^3.454.0",
    "@aws-sdk/client-sso-admin": "^3.454.0",
    "source-map-support": "^0.5.21",
    "dotenv": "^16.3.1"
  }
}
